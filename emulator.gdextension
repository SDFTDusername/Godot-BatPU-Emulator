[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust/target/debug/libbatpu_emulator.so"
linux.release.x86_64 =   "res://rust/target/release/libbatpu_emulator.so"
windows.debug.x86_64 =   "res://rust/target/debug/batpu_emulator.dll"
windows.release.x86_64 = "res://rust/target/release/batpu_emulator.dll"
macos.debug =            "res://rust/target/debug/libbatpu_emulator.dylib"
macos.release =          "res://rust/target/release/libbatpu_emulator.dylib"
macos.debug.arm64 =      "res://rust/target/debug/libbatpu_emulator.dylib"
macos.release.arm64 =    "res://rust/target/release/libbatpu_emulator.dylib"